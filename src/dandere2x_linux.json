{
  "dandere2x": {
    "usersettings": {
      "block_size": 20,
      "quality_minimum": 80,
      "waifu2x_type": "vulkan",
      "denoise_level": 0,
      "scale_factor": 2,
      "input_file": "../workspace/5sec.mkv",
      "output_file": "../workspace/5sec2x.mkv"
    },
    "developer_settings": {
      "realtime_encoding": {
        "realtime_encoding_enabled": true,
        "realtime_encoding_delete_files": false,
        "realtime_encoding_seconds_per_video": 1
      },
      "step_size": 8,
      "bleed": 1,
      "quality_moving_ratio": 0.55,
      "extension_type": ".jpg",
      "debug": true,
      "workspace_use_temp": false,
      "gui_delete_workspace_after": false,
      "workspace": "../workspace/default/",
      "dandere2x_cpp_dir": "../externals/dandere2x_cpp"
    }
  },
  "waifu2x_converter": {
    "waifu2x_converter_path": "..\\externals\\waifu2x-converter-cpp",
    "waifu2x_converter_file_name": "waifu2x-converter-cpp.exe",
    "output_options": {
      "--output-format": null,
      "--png-compression": null,
      "--image-quality": null,
      "--block-size": null,
      "--disable-gpu": null,
      "--force-OpenCL": true,
      "--processor": null,
      "--jobs": null,
      "--model-dir": null,
      "--mode": null,
      "--silent": true
    }
  },
  "waifu2x_ncnn_vulkan": {
    "waifu2x_ncnn_vulkan_path": "/usr/bin/",
    "waifu2x_ncnn_vulkan_file_name": "waifu2x-ncnn-vulkan",
    "output_options": {
        "-t": 256,
        "-model-path": null,
        "-gpu": null,
        "-load-proc-save_threads": null,
        "-verbose": null
    }
},
  "ffmpeg": {
    "ffmpeg_path": "ffmpeg",
    "ffprobe_path": "ffprobe",
    "-hwaccel": "none",
    "trim_video": {
      "time": {
        "-ss": null,
        "-to": null
      },
      "output_options": {
        "-map": 0,
        "-c": null
      }
    },
    "video_to_frames": {
      "output_options": {
        "-qscale:v": 2,
        "-pix_fmt": null,
        "-vf": [
          "noise=c1s=8:c0f=u"
        ]
      }
    },
    "frames_to_video": {
      "output_options": {
        "-vcodec": "libx264",
        "-crf": 17,
        "-b:v": null,
        "-pix_fmt": null,
        "-vf": [
          " pp=hb/vb/dr/fq|32, deband=range=22:blur=false"
        ]
      },
      "-y": true
    },
    "concat_videos": {
      "output_options": {
        "-codec": "copy",
        "-c": "copy"
      }
    },
    "migrating_tracks": {
      "output_options": {
        "-y": true,
        "-pix_fmt": null
      }
    }
  }
}
